<div class="grid">
  <div class="grid-left">
    <div class="admin__title">Organizations</div>

    {{#each model as |org|}}
      <div class="admin-side__card">
        <div class="user-org">{{ org.name }}</div>
        <div class="button-box">
          {{#link-to "admin.resources.detail" org.id class="btn" }}Details{{/link-to}}
          {{#link-to "admin.resources.edit" org.id class="btn" }}Edit{{/link-to}}
        </div>
      </div>
    {{/each}}

  </div>
  <div class="grid-right">{{outlet}}</div>
</div>
